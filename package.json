{
  "name": "twitter",
  "version": "1.0.0",
  "description": "this is twitter clone for team 6 BE subteam ",
  "main": "app.js",
  "scripts": {
    "devStart": "env-cmd -f ./app/config/dev.env nodemon server.js",
    "test": "env-cmd -f ./app/config/test.env jest --verbose --coverage --runInBand --force-exit --testTimeout=15000",
    "start-gendoc": "node swagger.js",
    "test:watch": "jest --watch",
    "jsdoc": "jsdoc -c jsdoc.json"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "node.js",
    "express",
    "jwt",
    "authentication",
    "mongodb"
  ],
  "author": "BE team 6",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "googleapis": "^99.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.10",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "onesignal-node": "^3.3.0",
    "pusher": "^5.1.0-beta",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "env-cmd": "^10.1.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.10",
    "node-mocks-http": "^1.11.0",
    "supertest": "^6.2.2",
    "swagger-autogen": "^2.20.5"
  }
}
